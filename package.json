{
  "name": "@ai-test/api-test-generator",
  "version": "1.0.0",
  "description": "Enterprise API test automation framework with OpenAPI integration",
  "main": "dist/index.js",
  "bin": {
    "api-test-gen": "./dist/cli.js"
  },
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts tests/**/*.ts --fix",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean",
    "prepare": "echo 'Skipping build for local deployment'",
    "deploy:local": "npm start",
    "security:scan": "node dist/security/security-scanner.js",
    "performance:test": "node dist/performance/performance-tester.js",
    "generate:tests": "node dist/cli/index.js generate"
  },
  "keywords": [
    "api",
    "testing",
    "automation",
    "openapi",
    "swagger",
    "typescript",
    "jest",
    "cli"
  ],
  "author": "AI API Test Automation Team",
  "license": "MIT",
  "devDependencies": {
    "@types/chokidar": "^1.7.5",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/eslint": "^9.6.1",
    "@types/express": "^5.0.3",
    "@types/jest": "^29.5.5",
    "@types/js-yaml": "^4.0.9",
    "@types/lodash": "^4.14.199",
    "@types/node": "^20.6.0",
    "@typescript-eslint/eslint-plugin": "^8.39.1",
    "@typescript-eslint/parser": "^8.39.1",
    "eslint": "^8.57.1",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.6.2",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "ajv": "^8.12.0",
    "axios": "^1.5.0",
    "chalk": "^4.1.2",
    "chokidar": "^4.0.3",
    "commander": "^11.0.0",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "inquirer": "^8.2.6",
    "js-yaml": "^4.1.0",
    "jsonschema": "^1.4.1",
    "lodash": "^4.17.21",
    "node-cron": "^3.0.2",
    "ora": "^5.4.1",
    "swagger-parser": "^10.0.3",
    "ts-morph": "^19.0.0",
    "winston": "^3.10.0",
    "yaml": "^2.3.2"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ai-test/api-test-generator.git"
  },
  "bugs": {
    "url": "https://github.com/ai-test/api-test-generator/issues"
  },
  "homepage": "https://github.com/ai-test/api-test-generator#readme"
}
