        return {{"Authorization": "Bearer environment_test_token"}}
    
    @pytest.fixture
    def env_backup(self):
        """Backup and restore environment variables"""
        original_env = os.environ.copy()
        yield
        # Restore original environment
        os.environ.clear()
        os.environ.update(original_env)
    
